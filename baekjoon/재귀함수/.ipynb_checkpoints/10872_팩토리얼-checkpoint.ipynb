    "# 재귀함수로 풀기\n",
    "def fac(num) :\n",
    "    if num <= 1 :\n",
    "        return 1\n",
    "    else :\n",
    "        return num * fac(num-1)\n",
    "    \n",
    "n = int(input())\n",
    "print(fac(n))"
   ]
  }
